<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <servlet>
        <servlet-name>Create</servlet-name>
        <servlet-class>webapp.Create</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Create</servlet-name>
        <url-pattern>/Create</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>AccountBalance</servlet-name>
        <servlet-class>webapp.AccountBalance</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AccountBalance</servlet-name>
        <url-pattern>/AccountBalance</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>FundTransfer</servlet-name>
        <servlet-class>webapp.FundTransfer</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>FundTransfer</servlet-name>
        <url-pattern>/FundTransfer</url-pattern>
    </servlet-mapping>
</web-app>